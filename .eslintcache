atch (error) {\r\n        continue;\r\n      }\r\n    }\r\n\r\n    console.log(\"Episode list\", likedEpisodesList);\r\n\r\n    if (likedEpisodesList.length) {\r\n      this.setState({ loading: false, episodes: likedEpisodesList });\r\n    }\r\n\r\n    if (likedEpisodesList.length === 0) {\r\n      this.setState({\r\n        emptyList: true,\r\n      });\r\n    }\r\n\r\n    try {\r\n      const likedEpisodeResponse = await getAllUserLikes();\r\n      console.log(\"Liked episodes\", likedEpisodeResponse.data.data);\r\n    } catch (error) {}\r\n  }\r\n\r\n  render() {\r\n    const { episodes } = { ...this.state };\r\n\r\n    const episodesList = episodes.map((episode, i) => (\r\n      <div className=\"episode-item\" key={episode._id}>\r\n        <div>\r\n          <h3>\r\n            <span style={{ opacity: \"0.5\" }}>{i + 1}</span> - {episode.title}\r\n          </h3>\r\n          <p>{episode.description}</p>\r\n          <p>{episode.likesCount} likes </p>\r\n        </div>\r\n        <div className=\"button-container\">\r\n          <button\r\n            onClick={() => this.props.playerLaunch(episode, episode.podcastId)}\r\n          >\r\n            {episodePlayIcon()} Play\r\n          </button>\r\n          {this.state.episodeDeleteDialog &&\r\n            this.state.episodeId === episode._id && (\r\n              <div className=\"delete-dialog\">\r\n                {this.state.episodeDeleting ? (\r\n                  \"Deleting...\"\r\n                ) : this.state.episodeDeleted ? (\r\n                  \"Deleted\"\r\n                ) : (\r\n                  <React.Fragment>\r\n                    <span>\r\n                      <i class=\"fas fa-info-circle\"></i>\r\n                    </span>\r\n                    <p>Are you sure?</p>\r\n                    <button\r\n                      onClick={(e) =>\r\n                        this.setState({ episodeDeleteDialog: false })\r\n                      }\r\n                    >\r\n                      No\r\n                    </button>\r\n                    <button\r\n                      onClick={(e) => this.handleEpisodeDelete(e, episode._id)}\r\n                    >\r\n                      Delete\r\n                    </button>\r\n                  </React.Fragment>\r\n                )}\r\n              </div>\r\n            )}\r\n        </div>\r\n        <hr />\r\n      </div>\r\n    ));\r\n    return (\r\n      <React.Fragment>\r\n        <div id=\"home\" className=\"favorites-page\">\r\n          {this.state.emptyList ? (\r\n            <React.Fragment>\r\n              <div className=\"error-container\">\r\n                {noSubIcon()}\r\n                <p>You dont have any favorites yet.</p>\r\n              </div>\r\n            </React.Fragment>\r\n          ) : (\r\n            <React.Fragment>\r\n              <h4>Here are your liked episodes</h4>\r\n              {this.state.loading ? <IconLoaderVariant1 /> : episodesList}\r\n            </React.Fragment>\r\n          )}\r\n        </div>\r\n      </React.Fragment>\r\n    );\r\n  }\r\n}\r\n\r\nexport { FavoritesPage };\r\n","C:\\Users\\User\\Desktop\\Projects\\press-play\\src\\api\\auth\\searchService\\index.js",[],"C:\\Users\\User\\Desktop\\Projects\\press-play\\src\\pages\\application\\searchPage\\index.js",["221","222"],"C:\\Users\\User\\Desktop\\Projects\\press-play\\src\\api\\auth\\getUserAvatar\\index.js",[],{"ruleId":"223","replacedBy":"224"},{"ruleId":"225","replacedBy":"226"},{"ruleId":"227","severity":1,"message":"228","line":2,"column":8,"nodeType":"229","messageId":"230","endLine":2,"endColumn":14},{"ruleId":"227","severity":1,"message":"231","line":6,"column":7,"nodeType":"229","messageId":"230","endLine":6,"endColumn":12},{"ruleId":"227","severity":1,"message":"232","line":5,"column":10,"nodeType":"229","messageId":"230","endLine":5,"endColumn":23},{"ruleId":"227","severity":1,"message":"233","line":4,"column":3,"nodeType":"229","messageId":"230","endLine":4,"endColumn":14},{"